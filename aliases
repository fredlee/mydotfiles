alias g='grep -i'  #case insensitive grep
alias f='find . -iname'
alias ducks='du -cks * | sort -rn|head -11' # Lists the size of all the folders and files
alias ll='ls -hl'
alias ..="cd .."
alias ...="cd ../.."
alias cc="cd ~/code/cnuapp"
alias ew="cd /export/web"
alias p4c="p4 client" 
alias p4o="p4 opened ./..."
alias p4ch="p4 changes -L ./..."
alias p4r="p4 resolve ./..."
alias p4ras="p4 resolve -as ./..."
alias p4d="p4 describe" 
alias p4s="p4 submit ./..." 
alias p4ms="p4 monitor show" 
alias tree="tree -afphugDFC"
alias screen='TERM=screen screen'

alias start_pg='pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start'
alias stop_pg='pg_ctl -D /usr/local/var/postgres stop -s -m fast'
alias tail_pg='tail -f /usr/local/var/postgres/server.log'

